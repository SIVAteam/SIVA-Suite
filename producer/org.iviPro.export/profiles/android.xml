<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<profile title="android" description="Template for Android devices"
	exportXml="true" exportSmil="false" exportFlashPlayer="false"
	exportHtmlPlayer="false" exportAudioExtensions="true"
	exportVideoExtensions="true" descriptorDirectory="XML"
	imageDirectory="pix" richPageDirectory="richpages" audioDirectory="audios"
	videoDirectory="videos" pdfDirectory="pdf">
	<audioVariants>
		<audioVariant title="default" description="The default audio variant.">
			<audioProfile audioContainer="mp3" audioCodec="libmp3lame"
				audioBitRate="128" sampleRate="44100" sampleSize="16" channels="1" />
		</audioVariant>
	</audioVariants>
	<videoVariants>
		<videoVariant title="default (fromerly large)"
			description="The default video variant.">
			<videoProfile videoContainer="mp4" videoCodec="libx264"
				videoBitRateType="original" videoBitRate="512" frameRate="-1.0"
				audioCodec="libfdk_aac"
				audioBitRate="128" sampleRate="48000" sampleSize="16" channels="2" videoCodecQuality="high"/>
		</videoVariant>
	</videoVariants>
</profile>
