<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<profile title="flash" description="Flash Export" exportXml="true"
	exportSmil="false" exportFlashPlayer="true" exportHtmlPlayer="false"
	exportAudioExtensions="true" exportVideoExtensions="true"
	descriptorDirectory="XML" imageDirectory="pix" richPageDirectory="richpages"
	audioDirectory="audios" videoDirectory="videos" pdfDirectory="pdf">
	<audioVariants>
		<audioVariant title="mp3" description="MP3">
			<audioProfile audioContainer="mp3" audioCodec="libmp3lame"
				audioBitRate="128" sampleRate="48000" sampleSize="16" channels="1" />
		</audioVariant>
		<audioVariant title="asf" description="ASF">
			<audioProfile audioContainer="asf" audioCodec="wmav2"
				audioBitRate="-1" sampleRate="-1" sampleSize="-1" channels="-1" />
		</audioVariant>
	</audioVariants>
	<videoVariants>
		<videoVariant title="default" description="The default video variant.">
			<videoProfile videoContainer="flv" videoCodec="flv"
				videoBitRateType="original" videoBitRate="512" frameRate="-1.0"
				audioCodec="libmp3lame"
				audioBitRate="320" sampleRate="-1" sampleSize="-1" channels="-1"/>
		</videoVariant>
	</videoVariants>
</profile>
